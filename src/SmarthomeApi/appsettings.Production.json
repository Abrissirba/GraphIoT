{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "SmarthomeDB": "Data Source=.\\SQLEXPRESS;Database=MyProdDatabaseName;Trusted_Connection=Yes;"
  },
  "TokenStoreConfig": {
    "ClassNameMapping": {
      "PersistingDigitalstromAuth": "digitalstrom.smarthomeapi",
      "ViessmannPlatformClient": "viessmann_platform",
      "ViessmannVitotrolClient": "viessmann_vitotrol",
      "SonnenPortalClient": "sonnen_portal",
      "WithingsClient": "withings",
      "DynDnsIpv4": "dyndns_ipv4",
      "DynDnsIpv6": "dyndns_ipv6"
    }
  },
  "AudiConnectConfig": {
    "Username": "<user>",
    "Password": "<pass>"
  },
  "DigitalstromConfig": {
    "PollingService": {
      "Name": "Digitalstrom",
      "TimerInterval": "5m"
    },
    "EventProcessor": {
      "ItemCollectionInterval": "5s",
      "DuplicateDetectionInterval": "1s"
    },
    "TokenAppId": "SmarthomeAPI",
    "DssUsername": "<user>",
    "DssPassword": "<pass>",
    "DssCertificate": "MIIC+jCCA<...base64cert...>g==",
    "DssUriLocal": "https://10.0.0.1:8080",
    "DssUriDsNet": "https://{CloudDssId}.digitalstrom.net:8080/",
    "DssUriCloudredir": "https://cloudredir.somedomain.tld/?adr={CloudDssId}&token={CloudredirToken}&path=",
    "CloudDssId": "<dssId>",
    "CloudredirToken": "<redirToken>",
    "UseCloudredir": false,
    "Proxy": "",
    "ProxyPort": ""
  },
  "NetatmoConfig": {
    "PollingService": {
      "Name": "Netatmo",
      "TimerInterval": "5m"
    },
    "Username": "<user>",
    "Password": "<pass>",
    "AppId": "<appId>",
    "AppSecret": "<appSecret>",
    "Scope": "read_station read_presence access_presence"
  },
  "SonnenConfig": {
    "PollingService": {
      "Name": "Sonnen",
      "TimerInterval": "5m"
    },
    "ClientId": "<appId>",
    "Username": "<user>",
    "Password": "<pass>"
  },
  "ViessmannConfig": {
    "PollingService": {
      "Name": "Viessmann",
      "TimerInterval": "5m"
    },
    "Username": "<user>",
    "Password": "<pass>",
    "VitotrolDeviceId": "<id>",
    "VitotrolInstallationId": "<id>",
    "PlattformInstallationId": "<id>",
    "PlattformGatewayId": "<id>",
    "PlattformApiClientId": "<appId>",
    "PlattformApiClientSecret": "<appSecret>"
  },
  "WithingsConfig": {
    "WithingsClientId": "<appId>",
    "WithingsClientSecret": "<appSecret>"
  },
  "TelegramConfig": {
    "BotId": "<botId>",
    "ChatId": "<chadId>",
    "WebhookBaseUri": "https://mydomain.tld/mybasepath",
    "WhitelistFirstname": "<firstname>",
    "WhitelistLastname": "<lastname>"
  }
}